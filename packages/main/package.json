{
  "name": "ai-sdk-agent-sdk",
  "version": "0.0.3",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./v2": {
      "bun": "./v2.ts",
      "types": "./dist/v2.d.ts",
      "import": "./dist/v2.js",
      "default": "./dist/v2.js"
    },
    "./bun": {
      "types": "./dist/index.d.ts",
      "bun": "./bun.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "index.ts",
    "v2.ts",
    "bun.ts",
    "internal/anthropic-option-warnings.ts",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf ./dist && bun build ./index.ts ./v2.ts --target=node --format=esm --outdir ./dist --external @ai-sdk/anthropic --external @ai-sdk/provider --external @ai-sdk/provider-utils --external @anthropic-ai/claude-agent-sdk",
    "test": "bun test",
    "test:e2e": "AI_SDK_AGENT_E2E=1 bun test __tests__/e2e --timeout 120000",
    "lint": "biome check . --write --unsafe",
    "prepublishOnly": "bun run build && cp ../../README.md ./README.md && sed -i '' 's|./packages/main/CHANGELOG.md|./CHANGELOG.md|g' ./README.md && sed -i '' 's|./example.png|https://raw.githubusercontent.com/zenyr/ai-sdk-agent-sdk/main/example.png|g' ./README.md",
    "postpublish": "rm -f ./README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenyr/ai-sdk-agent-sdk.git",
    "directory": "packages/main"
  },
  "homepage": "https://github.com/zenyr/ai-sdk-agent-sdk#readme",
  "bugs": {
    "url": "https://github.com/zenyr/ai-sdk-agent-sdk/issues"
  },
  "keywords": [
    "anthropic",
    "claude",
    "claude-code",
    "ai-sdk",
    "vercel-ai",
    "agent-sdk",
    "opencode",
    "llm",
    "streaming",
    "tool-use",
    "multimodal",
    "vision"
  ],
  "license": "MIT",
  "type": "module",
  "private": false,
  "devDependencies": {
    "@types/bun": "~1.3.9",
    "zod": "~4.3.6"
  },
  "peerDependencies": {
    "@ai-sdk/anthropic": "^3",
    "@ai-sdk/provider": "^3",
    "@ai-sdk/provider-utils": "^4",
    "@anthropic-ai/claude-agent-sdk": "^0",
    "typescript": "^5"
  }
}
